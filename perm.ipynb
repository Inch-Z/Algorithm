    "def perm(ls,k,m):\n",
    "    if k==m:\n",
    "        print(ls)\n",
    "    else:\n",
    "        for i in range(k,m+1):\n",
    "            ls[k],ls[i]=ls[i],ls[k]\n",
    "            perm(ls,k+1,m)\n",
    "            ls[k],ls[i]=ls[i],ls[k]      "
   ]
